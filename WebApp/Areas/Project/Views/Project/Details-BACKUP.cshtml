
@{
    ViewBag.Title = ProjectUI.ProjectUI.lblProjectDetails;


}


<input type="hidden" id="CreatedBy" name="CreatedBy" />
<input type="hidden" id="Id" name="Id" value="0" />
<input type="hidden" id="LoggedInUserId" name="LoggedInUserId" value="0" />
<input type="hidden" id="EmployeeId" name="LoggedInUserId" value="0" />
<div class="row">

</div>

<div class="row">
    <div class="col-sm-8">
        <div class="card ">

            <div class="card-header" style="background: #eceff1;">
                <div class="row">

                    <div class="col-sm-9">
                        <h6>
                            <span class=" txt-project-title" style="font-weight: bold;margin-right: 15px !important;"></span>
                        </h6>
                    </div>

                    <div class="col-sm-3 " style="text-align: end;">

                        <span class="badge">.Project No:</span>
                        <span class="badge badge-info    project-number">   </span>


                    </div>

                </div>





            </div>
            <div class="card-block" style="min-height: 49rem;">

                <div class="">
                    <div class="m-b-20">
                        <h6 class="sub-title m-b-15">Overview</h6>
                        <hr />
                        <p class="txt-description" style="overflow-x: auto;height: 43rem;">
                    </div>




                </div>
            </div>

            <div class="card-footer">

                <div class="row">

                    <div class="">
                        <div class="col-sm-4">
                            <span class="badge  ">Status:</span>
                            <select class="project_status_ddl" id="" style="background-color: #49626a; color: white; font: bold;">

                                <option value="Active">Active</option>
                                <option value="Closed">Closed</option>
                                <option value="Hold">Hold</option>
                                <option value="Completed">Completed</option>
                            </select>



                        </div>
                        <div class="col-sm-2"> </div>


                        <div class="col-sm-6 " style="text-align: end;">

                            <span class="badge">.Created By:</span>
                            <span class="badge badge-danger    text-project-created-by">   </span>


                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
    <div class="col-sm-4">
        <div class="card ">
            <div class="card-header" style="background: #eceff1;">
                <h6>
                    <span class="">Project details</span>
                </h6>

            </div>
            <div class="">

                <table class="table table-striped table-border">
                    <tbody>
                        <tr>

                            <td>Project Client:</td>
                            <td class="">
                                <b><span class="text-client-name"></span></b>
                            </td>

                        </tr>
                        <tr>
                            <td>Price</td>
                            <td class="text-price"></td>
                        </tr>

                        <tr>
                            <td>Started Date:</td>
                            <td>
                                <span class="badge badge-success text-startdate"></span>
                            </td>
                        </tr>
                        <tr>
                            <td>Completion Date:</td>
                            <td>
                                <span class="badge badge-danger text-enddate"></span>
                            </td>
                        </tr>
                        <tr>
                            <td></td>

                            <td class="">
                                <span class=" "></span>
                            </td>



                        </tr>

                        @*<tr>
                                <td>Status:</td>
                                <td class="text-end">Working</td>
                            </tr>*@
                        <tr>
                            <td>Progress</td>
                            <td class="text-progress-status">0 %</td>
                        </tr>
                    </tbody>
                </table>


            </div>

        </div>
    </div>
    <div class="col-sm-4">
        <div class="card ">
            <div class="card-header" style="background: #eceff1;">
                <h6><span class="">Project Attached Files</span></h6>

            </div>
            <div class="" style="height: 487px; overflow-x: auto; min-height: 24rem;">

                <table class="table table-striped table-border">
                    <tbody class="load-attachments">
                    </tbody>
                </table>


            </div>

        </div>
    </div>
</div>
