@* *********************                                           ******************** *@
@* ********************* LOAD TECHNICAL SECTION MODEL FOR UPLOADING DOCUMENT ******************** *@
@* *********************                                            ******************* *@
@* ********************* **************** BY |\/|ati ************************************* *@
<button type="button" style="display:none;" class="btn btn-primary close-technical-upload-modal" id="load-technical-section-model" data-toggle="modal" data-target="#modal-technical-section-upload-modal"></button>
<div class="modal fade" id="modal-technical-section-upload-modal" data-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <div class="row">
                    <div class="col-md-6">
                        <h5 class="modal-title" id="">@ProjectUI.ProjectUI.lblUploadDocument</h5>

                    </div>


                    <div class="col-md-6">
                        <div class="show-hide-employee-task-area" style="display: none;float: right;">
                            <div class="row">

                                <div class="col-md-12 @Resources.Common.PullRight show-hide-assign-start-datetime">
                                    <span class="">    @ProjectUI.ProjectUI.lblAssignDate </span>
                                    <b class=""><span class="assign-employee-start-date" id=""></span> </b>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12 @Resources.Common.PullRight show-hide-completion-start-datetime">
                                    <span class="">    @ProjectUI.ProjectUI.lblCompletionDate</span>
                                    <b class=" "><span class="assign-employee-end-date" id=""></span> </b>

                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12 @Resources.Common.PullRight show-hide-employee-start-datetime">

                                    <span class="">    @Resources.Global.lblStartDate</span>
                                    <b class="">
                                        <span class="employee-work-date" id=""></span>
                                        <span class="employee-work-time" id=""></span>
                                    </b>
                                    @*<button type="button" class="close clearField" data-dismiss="modal" aria-hidden="true">&times;</button>*@

                                </div>

                            </div>


                        </div>
                    </div>


                    @*<div class="col-md-4 @Resources.Common.PullRight show-hide-employee-start-datetime">
                            <span class="show-hide-employee-start-datetime" style="display:none;">    @Resources.Global.lblStartDate</span>
                            <b class="show-hide-employee-start-datetime" style="display:none;">
                                <span class="employee-work-date" id=""></span>
                                <span class="employee-work-time" id=""  ></span>
                            </b>


                        </div>*@





                </div>
            </div>
            <div class="modal-body">
                <form id="frmAddUpdate_TechnicalSection_Document" name="frmAddUpdate_TechnicalSection_Document" action="/services/Xtreme/multipart" method="post">

                    <input type="hidden" name="TechnicalSection_Document_Id" id="TechnicalSection_Document_Id" value="0">
                    <input type="hidden" name="TechnicalSection_Document_CreatedBy" id="TechnicalSection_Document_CreatedBy" value="0">
                    <input type="hidden" name="TechnicalSection_Document_ProjectId" id="TechnicalSection_Document_ProjectId" value="0">
                    <input type="hidden" name="type" id="type" value="Project_TechnicalSection_Document_Save">
                    <input type="hidden" name="TechnicalSection_Document_Language" id="TechnicalSection_Document_Language" class="Language" value="en-US">

                    @*<form id="frm_TechnicalSection_TransferDataModal">*@
                    <div class="row">
                        <div class="col-sm-6">
                            <div class="   typeddlRow">
                                <div id="TechnicalSection_ParentSection_container">
                                    <label class="col-form-label">@ProjectUI.ProjectUI.lblSection</label><span style="color:red"> *</span><br />
                                    <input class="form-control pname" id="Project_TechnicalSection_Parent_Type_DDL" readonly required>
                                    <input type="hidden" class="form-control pname" name="Project_TechnicalSection_Setup_SetupType_Id" id="Project_TechnicalSection_Setup_SetupType_Id" value="0">
                                </div>

                            </div>
                        </div>
                        <div class="col-sm-6">

                            <div class=" ">
                                <div id="TechnicalSection_SubSection_container">
                                    <label class="col-form-label"> @ProjectUI.ProjectUI.lblFile / @ProjectUI.ProjectUI.lblArea </label><span style="color:red"> *</span><br />
                                    <input type="text" class="form-control pname" name="Project_TechnicalSection_SetupDetailTypeDDL" id="Project_TechnicalSection_SetupDetailTypeDDL" required>
                                    <input type="hidden" class="form-control pname" name="Project_TechnicalSection_Entity_Id" id="Project_TechnicalSection_Entity_Id" value="0">
                                    <input type="hidden" class="form-control pname" name="Project_Technical_Section_Parent_Type_DDL_Text" id="Project_Technical_Section_Parent_Type_DDL_Text" value="0">
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="">
                        <div class="row">

                            <div class="col-sm-6">
                                <div class=" ">
                                    <label class="col-form-label">@Resources.Global.lblDocumentAttachment </label> <span style="color: red">*</span><br />
                                    <input type="file" class="form-control" name="TechnicalSectionGovernmentDocumentFile" required id="TechnicalSectionGovernmentDocumentFile" />
                                </div>
                            </div>
                            <div class="col-sm-6">  </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-3">
                                <div class=" ">
                                    <label class="col-form-label lbl-startDate">@ProjectUI.ProjectUI.lblIssueDate</label>   <br />
                                    <input class="form-control" type="date" id="TechnicalSection_Document_StartDate" name="TechnicalSection_Document_StartDate" />
                                </div>
                            </div>


                            @*<div class="col-sm-6">
                                    <div class=" ">
                                        <label class="col-form-label lbl-endDate">@ProjectUI.ProjectUI.lblExpiryDate</label> <span style="color:red">*</span> <br />

                                        <input class="form-control" type="date" id="TechnicalSection_Document_EndDate" name="TechnicalSection_Document_EndDate" required />
                                    </div>
                                </div>*@

                            <div class="col-sm-3">
                                <div class=" ">
                                    <label class="col-form-label">@ProjectUI.ProjectUI.lblExpiryDate</label>   <br />

                                    <input class="form-control" type="date" id="TechnicalSection_Document_EndDate" name="TechnicalSection_Document_EndDate" onchange="fnCheck_NoExpiry_TechnicalSection(this,'EndDate')" />
                                </div>
                            </div>
                            <div class="col-sm-3">
                                <div class=" ">

                                    <br />
                                    <br />
                                    <label class="btn " for="TechnicalSection_Document_NoExpiry_Call">
                                        <input checked type="radio" id="TechnicalSection_Document_NoExpiry_Call" style="width: 15px;height: 15px;" onchange="fnCheck_NoExpiry_TechnicalSection(this,'RadioButton')">
                                        <font class="columnTwo" style="font-size: initial;"> @ProjectUI.ProjectUI.lblNoExpiry </font>
                                    </label>
                                    <input type="hidden" id="TechnicalSection_Document_NoExpiry" style="width: 15px;height: 15px;" value="1" name="TechnicalSection_Document_NoExpiry">
                                </div>
                            </div>


                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <div class=" ">
                                    <label class="col-form-label">@Resources.Global.lblRemarks</label>
                                    <input class="form-control" id="TechnicalSection_Remarks" name="TechnicalSection_Remarks" />
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class=" ">
                                    <label class="col-form-label">@ProjectUI.ProjectUI.lblFor</label>
                                    <span style="color: red">*</span>
                                    <select class="form-control" id="TechnicalSection_comment_for_client_or_employee" name="TechnicalSection_comment_for_client_or_employee" required>
                                        <option value="">@ProjectUI.ProjectUI.lblSelect</option>
                                        <option selected value="Employee">@ProjectUI.ProjectUI.lblEmployee</option>
                                        <option value="Client">@ProjectUI.ProjectUI.lblClient</option>
                                        <option value="Both">@ProjectUI.ProjectUI.lblBoth</option>
                                        <option value="NoRemarks">@ProjectUI.ProjectUI.lblNoRemarks</option>
                                    </select>
                                </div>
                            </div>


                        </div>
                    </div>


                </form>
            </div>

            <div class="modal-footer">
                <div class="row">
                    <div class="col-md-12">


                        <div id="">

                            <button type="button" class="btn btn-danger   pull-right btnClose clearField" data-dismiss="modal"> @Resources.Global.lblClose</button>
                            <button type="button" id="btn-technical-section-upload-document" class="btn   btn-success  pull-right" value="Save"><i class="fa fa-save"></i> @Resources.Global.btnSave</button>

                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>

@* ********************* ********************* END **************************************** *@


