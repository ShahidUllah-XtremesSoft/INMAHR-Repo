
@{
    // ViewBag.Title = "Employee Profile";
    ViewBag.Title = HRModuleUI.HumanResourceUI.employeeProfile;

}


<input type="hidden" id="CreatedBy" name="CreatedBy" />
<input type="hidden" id="Id" name="Id" value="0" />
<input type="hidden" id="LoggedInUserId" name="LoggedInUserId" value="0" />
<input type="hidden" id="EmployeeId" name="EmployeeId" value="0" />
<div class="row">

</div>
