<button type="button" style="display:none;" class="btn btn-primary" id="load-technical-transfer-model" data-toggle="modal" data-target="#modal-technical-section-transfer-to-section"></button>
<div class="modal fade" id="modal-technical-section-transfer-to-section" data-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close clearField" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h5 class="modal-title" id="">@ProjectUI.ProjectUI.lblTransferTo</h5>
            </div>
            <div class="modal-body" style="height:auto;">
                <form id="frmTransferDataModal_TechnicalSection">
                    <div class="row">
                        <div class="col-sm-6">

                            <label class="col-form-label">@Resources.Global.lblDoYouWantToTransferToOtherMainSection :</label><br />
                        </div>
                        <div class="col-sm-2 pull-right">

                            <div class="form-group">
                                <div class="container">
                                    <select class="form-control main_Section_In_Sub_section_technical_transfer_modal" onchange="fnLoadOtherMainSection_In_TechnicalSection(this.value);">
                                        <option selected value="No">@Resources.Global.lblNo</option>
                                        <option value="Yes">@Resources.Global.lblYes</option>

                                    </select>

                                </div>
                            </div>
                        </div>
                        <div class="col-sm-4"> </div>
                    </div>


                    <div class="row div_showHide_Main_stepper">
                        <div class="col-sm-12">

                            <div class="form-group">
                                <div id="Project_load_other_main_section_stepper_in_TechnicalSection" style="background: oldlace;">  </div>
                            </div>
                        </div>
                    </div>


                    <div class="row">
                        <div class="col-sm-6">
                            <div class="form-group  typeddlRow">
                                @*<label class="col-form-label">.Transfer to </label><span style="color:red"> *</span><br />*@
                                <div id="container_parent_type_ddl_technical_transfer_modal">
                                    <input class="form-control pname" id="Project_Section_Parent_Type_DDL_technical_transfer_modal" required>
                                </div>
                                <input type="hidden" class="form-control pname" name="To_id_technical" id="To_id_technical" value="0">
                                <input type="hidden" class="form-control pname" id="From_id_technical" value="0">
                                <input type="hidden" class="form-control pname" id="Attachment_Id_technical_transfer_modal" value="0">
                                <input type="hidden" class="form-control pname" id="Grid_name_transfer_technical" value="0">
                            </div>

                        </div>
                        <div class="col-sm-6">

                            <div class="form-group">
                                <div id="container_child_type_ddl_technical_transfer_modal">
                                    @*<label class="col-form-label">@ProjectUI.ProjectUI.lblSubSection</label><span style="color:red"> *</span><br />*@
                                    <input type="text" class="form-control pname" name="Project_TechnicalSection_SetupDetailTypeDDL_technical_transfer_modal" id="Project_TechnicalSection_SetupDetailTypeDDL_technical_transfer_modal" required>
                                    <input type="hidden" class="form-control pname" name="Project_TechnicalSection_Entity_Id_transfer_modal" id="Project_TechnicalSection_Entity_Id_transfer_modal" value="0">

                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-6">
                            <div class="form-group  typeddlRow">
                                @*<label class="col-form-label">.Transfer to </label><span style="color:red"> *</span><br />*@
                                <div id="container">
                                    <input type="text" class="form-control pname" id="ApprovedOrReturned_TechnicalSection" name="ApprovedOrReturned_TechnicalSection" required>
                                </div>
                            </div>

                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-8">
                            <div class="form-group">
                                <label class="col-form-label">@Resources.Global.lblRemarks</label>
                                <input class="form-control" id="TechnicalSection_transfer_modal_Remarks" name="TechnicalSection_transfer_modal_Remarks" />
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="col-form-label">@ProjectUI.ProjectUI.lblFor</label>
                                <span style="color: red">*</span>
                                <select class="form-control" id="TechnicalSection_comment_for_client_or_employee_in_transfer_modal" name="TechnicalSection_comment_for_client_or_employee_in_transfer_modal" required>
                                    <option value="">@ProjectUI.ProjectUI.lblSelect</option>
                                    <option selected value="Employee">@ProjectUI.ProjectUI.lblEmployee</option>
                                    <option value="Client">@ProjectUI.ProjectUI.lblClient</option>
                                    <option value="Both">@ProjectUI.ProjectUI.lblBoth</option>
                                    <option value="NoRemarks">@ProjectUI.ProjectUI.lblNoRemarks</option>
                                </select>
                            </div>
                        </div>

                    </div>


                </form>
            </div>

            <div class="modal-footer">
                <div class="row">
                    <div class="col-md-12">


                        <div id="btnAreaShowHideOnConditionBase">

                            <button type="button" class="btn btn-danger   pull-right btnClose clearField_technical_section_transfer_modal" data-dismiss="modal">@Resources.Global.lblClose</button>
                            <button type="button" id="btn-technical-transfer" class="btn   btn-success  pull-right" value="Save"> @ProjectUI.ProjectUI.lblTransfer</button>

                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>

