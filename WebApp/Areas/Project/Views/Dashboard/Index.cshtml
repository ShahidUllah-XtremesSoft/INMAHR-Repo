<div class="row card">
    <div class="col-sm-12 col-lg-4">
        <div class="row">
            <div class="col-sm-6">
                <div class="callout callout-info">
                    <small class="text-muted">@ProjectUI.ProjectUI.lblTotalClients</small>
                    <br>
                    <strong class="h4" id="TotalClient">0</strong>

                </div>
            </div>
            <!--/.col-->
            <div class="col-sm-6" hidden>
                <div class="callout callout-danger">
                    <small class="text-muted">@ProjectUI.ProjectUI.lblProject</small>
                    <br>
                    <strong class="h4 TotalProjectCount" id="TotalProjectCount"> 0</strong>
                </div>
            </div>
            <!--/.col-->
        </div>
        <!--/.row-->
        <hr class="m-t-0">

    </div>
    <!--/.col-->
    <div class="col-sm-6 col-lg-4">
        <div class="row">
            <div class="col-sm-6">
                <div class="callout callout-warning">

                    <small class="text-muted">@ProjectUI.ProjectUI.lblActive @ProjectUI.ProjectUI.lblProject </small>
                    <br>
                    <strong class="h4 TotalActiveProjects">0</strong>
                </div>
            </div>
            <!--/.col-->
            <div class="col-sm-6">
                <div class="callout callout-success">
                    <small class="text-muted">@ProjectUI.ProjectUI.lblTemporary @ProjectUI.ProjectUI.lblProject </small>
                    <br>
                    <strong class="h4 TotalTemporaryProjects">0</strong>

                </div>
            </div>
            <!--/.col-->
        </div>
        <!--/.row-->
        <hr class="m-t-0">

    </div>
    <!--/.col-->
    <div class="col-sm-6 col-lg-4">
        <div class="row">
            <div class="col-sm-6" hidden>
                <div class="callout">
                    <small class="text-muted">@ProjectUI.ProjectUI.lblTotalContractors</small>
                    <br>

                    <strong class="h4" id="TotalContractor">0</strong>
                </div>
            </div>
            <!--/.col-->
            <div class="col-sm-3">
                <div class="callout callout-primary">
                    <small class="text-muted">@ProjectUI.ProjectUI.lblUrgent</small>
                    <br>
                    <strong class="h4 totalUrgent">0</strong>
                </div>
            </div>      <div class="col-sm-3">
                <div class="callout callout-primary">
                    <small class="text-muted">@ProjectUI.ProjectUI.lblVIP</small>
                    <br>
                    <strong class="h4 totalVIP">0</strong>
                </div>
            </div>
            <!--/.col-->
        </div>
        <!--/.row-->
        <hr class="m-t-0">

    </div>
    <!--/.col-->
</div>


